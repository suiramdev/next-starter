generator prisma {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

generator kysely {
    provider = "prisma-kysely"
    output = "../generated"
    fileName = "types.ts"
    enumFileName = "enums.ts"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}